{
  "name": "miseenplace",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Mise En Place",
    "manifest": "src/manifest.json",
    "main": "miseenplace.sketchplugin",
    "repository": "https://github.com/madwaro/MiseEnPlace.git",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "Carlos Madrigal <carlma@microsoft.com>"
}
